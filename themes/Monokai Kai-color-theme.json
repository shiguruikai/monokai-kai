      ],
      "settings": {
        "foreground": "#e6b422",
        "fontStyle": ""
      }
    },
    {
      "name": "[php] ドキュメント内で白くしない文字",
      "scope": "keyword.other.array.phpdoc.php",
      "settings": {
        "foreground": "#dfdfdf",
        "fontStyle": ""
      }
    },
    {
      "name": "[html] タグ記号",
      "scope": [
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#a8a8a8",
        "fontStyle": ""
      }
    },
    {
      "name": "[ts] デコレータ",
      "scope": ["source.ts meta.decorator.ts punctuation.decorator.ts"],
      "settings": {
        "foreground": "#e6b422",
        "fontStyle": ""
