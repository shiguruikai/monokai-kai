      "settings": {
        "foreground": "#ff6188",
        "fontStyle": ""
      }
    },
    {
      "name": "[shell] $記号",
      "scope": ["source.shell punctuation.definition.variable"],
      "settings": {
        "foreground": "#ab9df2",
        "fontStyle": ""
      }
    },
    {
      "name": "[shell] 特殊変数",
      "scope": [
        "source.shell variable.other.special",
        "source.shell variable.other.positional"
      ],
      "settings": {
        "foreground": "#ab9df2",
        "fontStyle": ""
      }
    },
    {
      "name": "[shell] 論理式の記号",
      "scope": ["source.shell punctuation.definition.logical-expression"],
      "settings": {
        "foreground": "#a9dc76",
        "fontStyle": ""
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.italic markup.bold ", "markup.bold markup.italic"],
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#78dce8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["fenced_code.block.language"],
      "settings": {
        "foreground": "#78dce8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "[markdown]",
      "scope": ["text.html.markdown punctuation.definition"],
      "settings": {
        "foreground": "#ff6188",
        "fontStyle": ""
      }
    },
    {
      "name": "[markdown]",
      "scope": ["text.html.markdown markup.inline.raw.string.markdown"],
      "settings": {
        "foreground": "#ffd866",
        "fontStyle": ""
      }
    },
    {
      "name": "[markdown]",
      "scope": ["text.html.markdown markup.underline.link.markdown"],
      "settings": {
        "foreground": "#007acc",
        "fontStyle": "underline"
      }
    },
    {
      "name": "[markdown]",
      "scope": ["text.html.markdown meta.separator.markdown"],
      "settings": {
        "foreground": "#a9dc76",
        "fontStyle": ""
      }
    },
    {
      "name": "[sql]",
      "scope": ["source.sql"],
      "settings": {
        "foreground": "#ace8ff",
        "fontStyle": ""
      }
    },
    {
      "name": "[sql] データーベース名、テーブル名",
      "scope": [
        "source.sql constant.other.database-name.sql",
        "source.sql constant.other.table-name.sql"
      ],
      "settings": {
        "foreground": "#78dce8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "[php] keyword",
