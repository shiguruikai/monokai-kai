      "settings": {
        "foreground": "#ff6188",
        "fontStyle": ""
      }
    },
    {
      "name": "[php] 定数",
      "scope": "source.php constant.other.class.php",
      "settings": {
        "foreground": "#ab9df2",
        "fontStyle": "italic"
      }
    },
    {
      "name": "[php] ヒアドキュメント記号",
